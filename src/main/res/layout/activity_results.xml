<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/textViewResultsName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Benchmark Statistics"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="2dp"
            android:background="@android:color/transparent" />

        <TextView
            android:id="@+id/textViewResultsTypeName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Collection - Method:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textViewResultsTypeValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="&lt;collectionType>"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_marginBottom="2dp"
            android:layout_marginTop="2dp"
            android:background="@android:color/transparent" />

        <TextView
            android:id="@+id/textViewResultsSizeName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Collection Size:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textViewResultsSizeValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="&lt;collectionSize>"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_marginBottom="2dp"
            android:layout_marginTop="2dp"
            android:background="@android:color/transparent" />

        <TextView
            android:id="@+id/textViewResultsLevelName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Parallelization Level:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textViewResultsLevelValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="&lt;parLevel>"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_marginBottom="2dp"
            android:layout_marginTop="2dp"
            android:background="@android:color/transparent" />

        <TextView
            android:id="@+id/textViewResultsItersName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Iterations"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textViewResultsItersValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="&lt;iters>"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="2dp"
            android:background="@android:color/transparent" />
			
		<TextView
            android:id="@+id/textViewResultsMultiName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Multiplier"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textViewResultsMultiValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="&lt;multi>"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="2dp"
            android:background="@android:color/transparent" />

        <TextView
            android:id="@+id/textViewResultsFastestName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Fastest Time:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textViewResultsFastestValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="&lt;time> ms"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/textViewResultsAllName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="All Times:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textViewResultsAllValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="false"
            android:text="&lt;times> ms"
            android:textAppearance="?android:attr/textAppearanceSmall" />
    </LinearLayout>

</ScrollView>